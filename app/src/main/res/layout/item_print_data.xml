<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_marginLeft="@dimen/dp_12"
    android:layout_marginRight="@dimen/dp_12"
    android:layout_marginTop="@dimen/dp_12"
    android:padding="@dimen/dp_12"
    android:background="@drawable/shape_5dp_59ff">

    <ImageView
        android:id="@+id/print_data_select"
        android:layout_width="@dimen/dp_20"
        android:layout_height="@dimen/dp_20"
        android:src="@mipmap/select_no"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />
    <TextView
        android:id="@+id/print_data_page"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="第1/6张"
        android:textSize="@dimen/sp_15"
        android:textColor="@color/color_3333"
        app:layout_constraintLeft_toRightOf="@id/print_data_select"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="@dimen/dp_8"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/app_name"
        android:textColor="@color/color_6666"
        android:textSize="@dimen/sp_14"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />
    <View
        android:id="@+id/print_data_line1"
        android:layout_width="0dp"
        android:layout_height="@dimen/dp_1"
        android:background="@color/color_e0e0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/print_data_page"
        android:layout_marginTop="@dimen/dp_8"
        />
    <TextView
        android:id="@+id/print_data_orderNum"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="运单号:1548785494545"
        android:textSize="@dimen/sp_14"
        android:textColor="@color/color_9999"
        app:layout_constraintTop_toBottomOf="@id/print_data_line1"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="@dimen/dp_8"
        />
    <TextView
        android:id="@+id/print_data_start"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="双流百家胡蛋黄酥"
        android:textColor="@color/color_3333"
        android:textSize="@dimen/sp_16"
        app:layout_constraintHorizontal_chainStyle="packed"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/print_data_orderNum"
        app:layout_constraintRight_toLeftOf="@+id/print_data_img2"
        android:layout_marginTop="@dimen/dp_11"
        android:lines="1"
        android:ellipsize="end"
        />
    <ImageView
        android:id="@+id/print_data_img2"
        android:layout_width="@dimen/dp_29"
        android:layout_height="@dimen/dp_12"
        android:src="@mipmap/arrows_double"
        app:layout_constraintLeft_toRightOf="@id/print_data_start"
        app:layout_constraintTop_toBottomOf="@id/print_data_orderNum"
        app:layout_constraintRight_toLeftOf="@+id/print_data_end"
        android:layout_marginTop="@dimen/dp_17"
        android:layout_marginLeft="@dimen/dp_4"
        />
    <TextView
        android:id="@+id/print_data_end"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="乐山大佛交底书"
        android:textColor="@color/color_3333"
        android:textSize="@dimen/sp_16"
        app:layout_constraintLeft_toRightOf="@id/print_data_img2"
        app:layout_constraintTop_toBottomOf="@id/print_data_orderNum"
        app:layout_constraintRight_toLeftOf="@+id/print_data_qr"
        android:layout_marginTop="@dimen/dp_11"
        android:layout_marginLeft="@dimen/dp_4"
        android:layout_marginRight="@dimen/dp_8"
        android:lines="1"
        android:ellipsize="end"
        />
    <TextView
        android:id="@+id/print_data_img4"
        android:layout_width="@dimen/dp_18"
        android:layout_height="@dimen/dp_18"
        android:text="收:"
        android:textColor="@color/color_6666"
        android:textSize="@dimen/sp_14"
        app:layout_constraintTop_toBottomOf="@id/print_data_end"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="@dimen/dp_15"
        />
    <TextView
        android:id="@+id/print_data_receive_man"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="李文冷冻"
        android:textColor="@color/color_3333"
        android:textSize="@dimen/sp_16"
        android:lines="1"
        android:maxEms="4"
        android:ellipsize="end"
        app:layout_constraintLeft_toRightOf="@id/print_data_img4"
        app:layout_constraintTop_toBottomOf="@id/print_data_end"
        android:layout_marginTop="@dimen/dp_13"
        android:layout_marginLeft="@dimen/dp_10"
        />
    <TextView
        android:id="@+id/print_data_receive_phone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="12535234231"
        android:textSize="@dimen/sp_14"
        android:textColor="@color/color_3333"
        app:layout_constraintLeft_toRightOf="@id/print_data_receive_man"
        app:layout_constraintTop_toBottomOf="@id/print_data_end"
        android:layout_marginTop="@dimen/dp_15"
        android:layout_marginLeft="@dimen/dp_8"
        />

    <TextView
        android:id="@+id/print_data_receive_locate"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="白家市场5栋28号冷冻室"
        android:textColor="@color/color_9999"
        android:textSize="@dimen/sp_14"
        app:layout_constraintLeft_toRightOf="@id/print_data_img4"
        app:layout_constraintTop_toBottomOf="@id/print_data_receive_man"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="@dimen/dp_10"
        android:layout_marginLeft="@dimen/dp_8"
        />
    <TextView
        android:id="@+id/print_data_word3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/type"
        android:textColor="@color/color_6666"
        android:textSize="@dimen/sp_14"
        app:layout_constraintLeft_toRightOf="@id/print_data_img4"
        app:layout_constraintTop_toBottomOf="@id/print_data_receive_locate"
        android:layout_marginTop="@dimen/dp_6"
        android:layout_marginLeft="@dimen/dp_10"
        />
    <TextView
        android:id="@+id/print_data_type"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="冷冻生鲜"
        android:textSize="@dimen/sp_14"
        android:textColor="@color/color_3333"
        app:layout_constraintLeft_toRightOf="@id/print_data_word3"
        app:layout_constraintTop_toBottomOf="@id/print_data_receive_locate"
        android:layout_marginTop="@dimen/dp_6"
        android:layout_marginLeft="@dimen/dp_8"
        />
    <TextView
        android:id="@+id/print_data_word4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/total"
        android:textColor="@color/color_6666"
        android:textSize="@dimen/sp_14"
        app:layout_constraintLeft_toRightOf="@id/print_data_img4"
        app:layout_constraintTop_toBottomOf="@id/print_data_word3"
        android:layout_marginTop="@dimen/dp_6"
        android:layout_marginLeft="@dimen/dp_10"
        />
    <TextView
        android:id="@+id/print_data_number"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="6件"
        android:textSize="@dimen/sp_14"
        android:textColor="@color/color_3333"
        app:layout_constraintLeft_toRightOf="@id/print_data_word4"
        app:layout_constraintTop_toBottomOf="@id/print_data_type"
        android:layout_marginTop="@dimen/dp_6"
        android:layout_marginLeft="@dimen/dp_8"
        />
    <TextView
        android:id="@+id/print_data_total_detail"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="(大件x1  小件x2  中件x3  中小件x5）"
        android:textColor="@color/color_6666"
        android:textSize="@dimen/sp_12"
        app:layout_constraintLeft_toRightOf="@id/print_data_number"
        app:layout_constraintTop_toBottomOf="@id/print_data_type"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="@dimen/dp_8"
        />
    <ImageView
        android:id="@+id/print_data_qr"
        android:layout_width="@dimen/dp_90"
        android:layout_height="@dimen/dp_90"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/print_data_line1"
        android:layout_marginTop="@dimen/dp_6"
        android:background="@color/color_3333"
        android:scaleType="fitXY"
        />
</android.support.constraint.ConstraintLayout>