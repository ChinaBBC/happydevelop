<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".logistics.activity.QuantitySetActivity">
    <include
        layout="@layout/a_common_title"></include>

    <LinearLayout
        android:id="@+id/quantity_logistic_product"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_54"
        android:orientation="horizontal"
        android:gravity="center_vertical"
        android:paddingLeft="@dimen/dp_15"
        android:paddingRight="@dimen/dp_15"
        >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/logistics_product"
            android:textColor="@color/color_3333"
            android:textSize="@dimen/sp_14"
            />
        <TextView
            android:id="@+id/quantity_logistic_productText"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:text="@string/please_choice"
            android:textSize="@dimen/sp_14"
            android:textColor="@color/color_9999"
            android:gravity="right"
            />
        <ImageView
            android:layout_width="@dimen/dp_10"
            android:layout_height="@dimen/dp_10"
            android:src="@mipmap/in"
            android:layout_marginLeft="@dimen/dp_6"
            />
    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_1"
        android:background="@color/color_e0e0"
        android:layout_marginLeft="@dimen/dp_15"
        android:layout_marginRight="@dimen/dp_15"
        ></View>
    <LinearLayout
        android:id="@+id/quantity_logistic_company"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_54"
        android:orientation="horizontal"
        android:gravity="center_vertical"
        android:paddingLeft="@dimen/dp_15"
        android:paddingRight="@dimen/dp_15"
        >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/logistics_company_b"
            android:textColor="@color/color_3333"
            android:textSize="@dimen/sp_14"
            />
        <TextView
            android:id="@+id/quantity_logistic_companyText"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:text="@string/please_choice"
            android:textSize="@dimen/sp_14"
            android:textColor="@color/color_9999"
            android:gravity="right"
            />
        <ImageView
            android:layout_width="@dimen/dp_10"
            android:layout_height="@dimen/dp_10"
            android:src="@mipmap/in"
            android:layout_marginLeft="@dimen/dp_6"
            />
    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_1"
        android:background="@color/color_e0e0"
        android:layout_marginLeft="@dimen/dp_15"
        android:layout_marginRight="@dimen/dp_15"
        ></View>
    <LinearLayout
        android:id="@+id/quantity_logistic_diver"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_54"
        android:orientation="horizontal"
        android:gravity="center_vertical"
        android:paddingLeft="@dimen/dp_15"
        android:paddingRight="@dimen/dp_15"
        >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/receive_diver"
            android:textColor="@color/color_3333"
            android:textSize="@dimen/sp_14"
            />
        <TextView
            android:id="@+id/quantity_logistic_diverText"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:text="@string/please_choice"
            android:textSize="@dimen/sp_14"
            android:textColor="@color/color_9999"
            android:gravity="right"
            />
        <ImageView
            android:layout_width="@dimen/dp_10"
            android:layout_height="@dimen/dp_10"
            android:src="@mipmap/in"
            android:layout_marginLeft="@dimen/dp_6"
            />
    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_1"
        android:background="@color/color_e0e0"
        android:layout_marginLeft="@dimen/dp_15"
        android:layout_marginRight="@dimen/dp_15"
        ></View>
    <LinearLayout
        android:id="@+id/quantity_logistic_carId"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_54"
        android:orientation="horizontal"
        android:gravity="center_vertical"
        android:paddingLeft="@dimen/dp_15"
        android:paddingRight="@dimen/dp_15"
        >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/car_id"
            android:textColor="@color/color_3333"
            android:textSize="@dimen/sp_14"
            />
        <TextView
            android:id="@+id/quantity_logistic_carIdText"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:text="@string/please_choice"
            android:textSize="@dimen/sp_14"
            android:textColor="@color/color_9999"
            android:gravity="right"
            />
        <ImageView
            android:layout_width="@dimen/dp_10"
            android:layout_height="@dimen/dp_10"
            android:src="@mipmap/in"
            android:layout_marginLeft="@dimen/dp_6"
            />
    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_1"
        android:background="@color/color_e0e0"
        android:layout_marginLeft="@dimen/dp_15"
        android:layout_marginRight="@dimen/dp_15"
        ></View>
    <LinearLayout
        android:id="@+id/quantity_logistic_list"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_54"
        android:orientation="horizontal"
        android:gravity="center_vertical"
        android:paddingLeft="@dimen/dp_15"
        android:paddingRight="@dimen/dp_15"
        >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/class_list"
            android:textColor="@color/color_3333"
            android:textSize="@dimen/sp_14"
            />
        <TextView
            android:id="@+id/quantity_logistic_listText"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content"
            android:text="@string/please_choice"
            android:textSize="@dimen/sp_14"
            android:textColor="@color/color_9999"
            android:gravity="right"
            />
        <ImageView
            android:layout_width="@dimen/dp_10"
            android:layout_height="@dimen/dp_10"
            android:src="@mipmap/in"
            android:layout_marginLeft="@dimen/dp_6"
            />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        android:background="@color/color_f0f0"
        android:orientation="horizontal"
        >
        <Button
            android:id="@+id/quantity_logistic_complete"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_40"
            android:text="@string/complete_btn"
            android:textColor="@color/white"
            android:textSize="@dimen/sp_14"
            android:background="@drawable/shape_5dp_a45b"
            android:layout_marginRight="@dimen/dp_15"
            android:layout_marginLeft="@dimen/dp_15"
            android:layout_marginTop="@dimen/dp_24"
            />
    </LinearLayout>

</LinearLayout>