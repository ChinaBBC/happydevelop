<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginLeft="@dimen/dp_12"
    android:layout_marginRight="@dimen/dp_12"
    android:layout_marginTop="@dimen/dp_10"
    android:padding="@dimen/dp_10"
    android:background="@drawable/shape_2dp_59ff"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <View
        android:id="@+id/skeleton_order_time"
        android:layout_width="@dimen/dp_140"
        android:layout_height="@dimen/dp_20"
        android:background="@color/color_9999"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        />
    <View
        android:id="@+id/skeleton_order_status"
        android:layout_width="@dimen/dp_60"
        android:layout_height="@dimen/dp_20"
        android:background="@color/color_9999"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />
    <View
        android:id="@+id/skeleton_order_line1"
        android:layout_width="0dp"
        android:layout_height="@dimen/dp_1"
        android:background="@color/color_9999"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/skeleton_order_time"
        android:layout_marginTop="@dimen/dp_10"
        />
    <View
        android:id="@+id/skeleton_order_order"
        android:layout_width="@dimen/dp_140"
        android:layout_height="@dimen/dp_20"
        android:background="@color/color_9999"
        app:layout_constraintTop_toBottomOf="@id/skeleton_order_line1"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="@dimen/dp_10"
        />
    <View
        android:id="@+id/skeleton_order_way"
        android:layout_width="@dimen/dp_66"
        android:layout_height="@dimen/dp_20"
        android:background="@color/color_9999"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/skeleton_order_line1"
        android:layout_marginTop="@dimen/dp_12"
        />
    <View
        android:id="@+id/skeleton_order_img1"
        android:layout_width="@dimen/dp_20"
        android:layout_height="@dimen/dp_20"
        android:background="@color/color_9999"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/skeleton_order_order"
        android:layout_marginTop="@dimen/dp_6"
        />
    <View
        android:id="@+id/skeleton_order_shop"
        android:layout_width="@dimen/dp_90"
        android:layout_height="@dimen/dp_20"
        android:background="@color/color_9999"
        app:layout_constraintLeft_toRightOf="@id/skeleton_order_img1"
        app:layout_constraintTop_toBottomOf="@id/skeleton_order_order"
        android:layout_marginTop="@dimen/dp_6"
        android:layout_marginLeft="@dimen/dp_4"
        />
    <View
        android:id="@+id/skeleton_order_lines"
        android:layout_width="@dimen/dp_160"
        android:layout_height="@dimen/dp_20"
        android:background="@color/color_9999"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/skeleton_order_way"
        android:layout_marginTop="@dimen/dp_6"
        />
    <View
        android:id="@+id/skeleton_order_img2"
        android:layout_width="@dimen/dp_20"
        android:layout_height="@dimen/dp_20"
        android:background="@color/color_9999"
        app:layout_constraintTop_toBottomOf="@id/skeleton_order_img1"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="@dimen/dp_6"
        />
    <View
        android:id="@+id/skeleton_order_phone"
        android:layout_width="@dimen/dp_90"
        android:layout_height="@dimen/dp_20"
        android:background="@color/color_9999"
        app:layout_constraintTop_toBottomOf="@id/skeleton_order_shop"
        app:layout_constraintLeft_toRightOf="@id/skeleton_order_img2"
        android:layout_marginTop="@dimen/dp_6"
        android:layout_marginLeft="@dimen/dp_4"
        />
    <View
        android:id="@+id/skeleton_order_number"
        android:layout_width="@dimen/dp_90"
        android:layout_height="@dimen/dp_20"
        android:background="@color/color_9999"
        app:layout_constraintTop_toBottomOf="@id/skeleton_order_lines"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="@dimen/dp_2"
        />
    <TextView
        android:id="@+id/skeleton_order_address"
        android:layout_width="@dimen/dp_110"
        android:layout_height="@dimen/dp_20"
        android:background="@color/color_9999"
        app:layout_constraintLeft_toRightOf="@id/skeleton_order_img2"
        app:layout_constraintTop_toBottomOf="@id/skeleton_order_phone"
        android:layout_marginLeft="@dimen/dp_4"
        android:layout_marginTop="@dimen/dp_6"
        />
    <TextView
        android:id="@+id/skeleton_order_pay"
        android:layout_width="@dimen/dp_90"
        android:layout_height="@dimen/dp_20"
        android:background="@color/color_9999"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/skeleton_order_number"
        android:layout_marginTop="@dimen/dp_6"
        />
</android.support.constraint.ConstraintLayout>