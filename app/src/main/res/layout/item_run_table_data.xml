<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_marginTop="@dimen/dp_10"
    android:layout_marginRight="@dimen/dp_12"
    android:layout_marginLeft="@dimen/dp_12"
    android:background="@drawable/shape_2dp_59ff"
    android:padding="@dimen/dp_10">

    <TextView
        android:id="@+id/run_table_data_sendWay"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="同城配送"
        android:textColor="@color/color_703f"
        android:textSize="@dimen/sp_12"
        android:padding="@dimen/dp_2"
        android:background="@drawable/shape_3dp_703f"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />
    <TextView
        android:id="@+id/run_table_data_number"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="序号1"
        android:textSize="@dimen/sp_14"
        android:textColor="@color/color_6666"
        app:layout_constraintLeft_toRightOf="@id/run_table_data_sendWay"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="@dimen/dp_6"
        android:layout_marginTop="@dimen/dp_2"
        />
    <TextView
        android:id="@+id/run_table_data_company"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="物流公司:齐天大圣"
        android:textColor="@color/color_3333"
        android:textSize="@dimen/sp_14"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="@dimen/dp_2"
        />
    <TextView
        android:id="@+id/run_table_data_bill"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="20单"
        android:textColor="@color/color_3333"
        android:textSize="@dimen/sp_16"
        android:gravity="center"
        app:layout_constraintTop_toBottomOf="@id/run_table_data_number"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@id/run_table_data_fee"
        android:layout_marginTop="@dimen/dp_10"
        />
    <TextView
        android:id="@+id/run_table_data_fee"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="120元"
        android:textColor="@color/color_3333"
        android:textSize="@dimen/sp_16"
        android:gravity="center"
        app:layout_constraintTop_toBottomOf="@id/run_table_data_number"
        app:layout_constraintLeft_toRightOf="@id/run_table_data_bill"
        app:layout_constraintRight_toLeftOf="@id/run_table_data_brokerage"
        android:layout_marginTop="@dimen/dp_10"
        />
    <TextView
        android:id="@+id/run_table_data_brokerage"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="200元"
        android:textColor="@color/color_3333"
        android:textSize="@dimen/sp_16"
        android:gravity="center"
        app:layout_constraintTop_toBottomOf="@id/run_table_data_number"
        app:layout_constraintLeft_toRightOf="@id/run_table_data_fee"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="@dimen/dp_10"
        />
    <TextView
        android:id="@+id/run_table_data_word2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/way_bill_number"
        android:textSize="@dimen/sp_12"
        android:textColor="@color/color_3333"
        android:gravity="center"
        app:layout_constraintTop_toBottomOf="@id/run_table_data_bill"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@id/run_table_data_word3"
        android:layout_marginTop="@dimen/dp_4"
        />
    <TextView
        android:id="@+id/run_table_data_word3"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/real_pay"
        android:textSize="@dimen/sp_12"
        android:textColor="@color/color_3333"
        android:gravity="center"
        app:layout_constraintTop_toBottomOf="@id/run_table_data_fee"
        app:layout_constraintLeft_toRightOf="@id/run_table_data_word2"
        app:layout_constraintRight_toLeftOf="@id/run_table_data_word4"
        android:layout_marginTop="@dimen/dp_4"
        />
    <TextView
        android:id="@+id/run_table_data_word4"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/brokerage"
        android:textSize="@dimen/sp_12"
        android:textColor="@color/color_3333"
        android:gravity="center"
        app:layout_constraintTop_toBottomOf="@id/run_table_data_brokerage"
        app:layout_constraintLeft_toRightOf="@id/run_table_data_word3"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="@dimen/dp_4"
        />
</android.support.constraint.ConstraintLayout>