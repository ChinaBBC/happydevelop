<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@drawable/shape_shadow"
    android:layout_marginTop="@dimen/dp_10"
    android:layout_marginLeft="@dimen/dp_12"
    android:layout_marginRight="@dimen/dp_12"
    android:padding="@dimen/dp_14">

    <TextView
        android:id="@+id/lines_manage_sendType"
        android:layout_width="@dimen/dp_55"
        android:layout_height="@dimen/dp_18"
        android:text="城市配送"
        android:textSize="@dimen/sp_12"
        android:textColor="@color/color_703f"
        android:gravity="center"
        android:background="@drawable/shape_3dp_703f"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="@dimen/dp_2"
        />
    <TextView
        android:id="@+id/lines_manage_start"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="垂搜和"
        android:textColor="@color/color_3333"
        android:textSize="@dimen/sp_16"
        app:layout_constraintLeft_toRightOf="@id/lines_manage_sendType"
        app:layout_constraintRight_toLeftOf="@+id/lines_manage_img1"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="@dimen/dp_6"
        android:lines="1"
        android:ellipsize="end"
        android:maxLength="8"
        />
    <ImageView
        android:id="@+id/lines_manage_img1"
        android:layout_width="@dimen/dp_17"
        android:layout_height="@dimen/dp_17"
        android:src="@mipmap/arrows_short"
        app:layout_constraintLeft_toRightOf="@id/lines_manage_start"
        app:layout_constraintRight_toLeftOf="@+id/lines_manage_end"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="@dimen/dp_6"
        android:layout_marginTop="@dimen/dp_3"
        />
    <TextView
        android:id="@+id/lines_manage_end"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="是多少还好客户"
        android:textSize="@dimen/sp_16"
        android:textColor="@color/color_3333"
        app:layout_constraintLeft_toRightOf="@id/lines_manage_img1"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/lines_manage_pay_way"
        android:layout_marginLeft="@dimen/dp_6"
        android:lines="1"
        android:ellipsize="end"
        />
    <TextView
        android:id="@+id/lines_manage_pay_way"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="到付"
        android:textColor="@color/color_6c00"
        android:textSize="@dimen/sp_14"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="@dimen/dp_3"
        />
    <View
        android:id="@+id/lines_manage_point1"
        android:layout_width="@dimen/dp_7"
        android:layout_height="@dimen/dp_7"
        android:background="@color/color_6c00"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/lines_manage_start"
        android:layout_marginTop="@dimen/dp_18"
        android:visibility="gone"
        />
    <View
        android:id="@+id/lines_manage_point2"
        android:layout_width="@dimen/dp_7"
        android:layout_height="@dimen/dp_7"
        android:background="@color/blue"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/lines_manage_locate1"
        android:layout_marginTop="@dimen/dp_19"
        android:visibility="gone"
        />
    <TextView
        android:id="@+id/lines_manage_word1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/start"
        android:textSize="@dimen/sp_14"
        android:textColor="@color/black"
        app:layout_constraintTop_toBottomOf="@id/lines_manage_start"
        app:layout_constraintLeft_toRightOf="@id/lines_manage_point1"
        android:layout_marginTop="@dimen/dp_12"
        android:layout_marginLeft="@dimen/dp_6"
        android:visibility="gone"
        />
    <TextView
        android:id="@+id/lines_manage_word2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/end"
        android:textSize="@dimen/sp_14"
        android:textColor="@color/black"
        app:layout_constraintTop_toBottomOf="@id/lines_manage_locate1"
        app:layout_constraintLeft_toRightOf="@id/lines_manage_point2"
        android:layout_marginTop="@dimen/dp_12"
        android:layout_marginLeft="@dimen/dp_6"
        android:visibility="gone"
        />
    <TextView
        android:id="@+id/lines_manage_locate1"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="dsdsdsdddadsdasd"
        android:textColor="@color/color_9999"
        android:textSize="@dimen/sp_14"
        app:layout_constraintLeft_toRightOf="@id/lines_manage_word1"
        app:layout_constraintTop_toBottomOf="@id/lines_manage_start"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="@dimen/dp_12"
        android:layout_marginLeft="@dimen/dp_6"
        android:visibility="gone"
        />
    <TextView
        android:id="@+id/lines_manage_locate2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="dsdsdsdddadsdasd"
        android:textColor="@color/color_9999"
        android:textSize="@dimen/sp_14"
        app:layout_constraintLeft_toRightOf="@id/lines_manage_word1"
        app:layout_constraintTop_toBottomOf="@id/lines_manage_locate1"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="@dimen/dp_12"
        android:layout_marginLeft="@dimen/dp_6"
        android:visibility="gone"
        />
    <TextView
        android:id="@+id/lines_manage_remark"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="备注：请一定送货到收货人手里！谢谢！"
        android:textColor="@color/color_3333"
        android:textSize="@dimen/sp_12"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/lines_manage_locate2"
        app:layout_constraintRight_toRightOf="parent"
        android:padding="@dimen/dp_10"
        android:layout_marginTop="@dimen/dp_10"
        android:background="@color/color_f5f1"
        />
    <LinearLayout
        android:id="@+id/lines_manage_fee"
        android:layout_width="@dimen/dp_85"
        android:layout_height="@dimen/dp_30"
        android:orientation="horizontal"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/lines_manage_remark"
        android:layout_marginTop="@dimen/dp_15"
        android:background="@drawable/shape_5dp_a45b"
        android:gravity="center"
        >
        <ImageView
            android:layout_width="@dimen/dp_14"
            android:layout_height="@dimen/dp_14"
            android:src="@mipmap/fee_standard"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/fee_standard"
            android:textSize="@dimen/sp_14"
            android:textColor="@color/white"
            android:layout_marginLeft="@dimen/dp_2"
            />
    </LinearLayout>
    <LinearLayout
        android:id="@+id/lines_manage_manage"
        android:layout_width="@dimen/dp_85"
        android:layout_height="@dimen/dp_30"
        android:orientation="horizontal"
        app:layout_constraintRight_toLeftOf="@id/lines_manage_fee"
        app:layout_constraintTop_toBottomOf="@id/lines_manage_remark"
        android:layout_marginTop="@dimen/dp_15"
        android:layout_marginRight="@dimen/dp_6"
        android:background="@drawable/shape_5dp_a47f"
        android:gravity="center"
        >
        <ImageView
            android:layout_width="@dimen/dp_14"
            android:layout_height="@dimen/dp_14"
            android:src="@mipmap/classes_manage_l"
            />
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/classes_manage"
            android:textSize="@dimen/sp_14"
            android:textColor="@color/white"
            android:layout_marginLeft="@dimen/dp_2"
            />
    </LinearLayout>
</android.support.constraint.ConstraintLayout>