<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_marginLeft="@dimen/dp_12"
    android:layout_marginRight="@dimen/dp_12"
    android:layout_marginTop="@dimen/dp_12"
    android:padding="@dimen/dp_12"
    android:background="@drawable/shape_5dp_59ff">

    <ImageView
        android:id="@+id/print_header_img1"
        android:layout_width="@dimen/dp_20"
        android:layout_height="@dimen/dp_20"
        android:src="@mipmap/select_yes_solid"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />
    <TextView
        android:id="@+id/print_header_word1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/new_client_back"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />
    <TextView
        android:id="@+id/print_header_line1"
        android:layout_width="0dp"
        android:layout_height="@dimen/dp_1"
        android:background="@color/color_e0e0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/print_header_word1"
        android:layout_marginTop="@dimen/dp_10"
        />
    <TextView
        android:id="@+id/print_header_start"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="双流百家"
        android:textColor="@color/color_3333"
        android:textSize="@dimen/sp_16"
        app:layout_constraintHorizontal_chainStyle="packed"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/print_header_line1"
        app:layout_constraintRight_toLeftOf="@id/print_header_img2"
        android:layout_marginTop="@dimen/dp_11"
        />
    <ImageView
        android:id="@+id/print_header_img2"
        android:layout_width="@dimen/dp_29"
        android:layout_height="@dimen/dp_12"
        android:src="@mipmap/arrows_double"
        app:layout_constraintLeft_toRightOf="@id/print_header_start"
        app:layout_constraintTop_toBottomOf="@id/print_header_line1"
        app:layout_constraintRight_toLeftOf="@id/print_header_end"
        android:layout_marginTop="@dimen/dp_17"
        android:layout_marginLeft="@dimen/dp_4"
        />
    <TextView
        android:id="@+id/print_header_end"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="乐山大佛"
        android:textColor="@color/color_3333"
        android:textSize="@dimen/sp_16"
        app:layout_constraintLeft_toRightOf="@id/print_header_img2"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/print_header_line1"
        android:layout_marginTop="@dimen/dp_11"
        android:layout_marginLeft="@dimen/dp_4"
        />
    <TextView
        android:id="@+id/print_header_orderNum"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="运单号:1548785494545"
        android:textSize="@dimen/sp_14"
        android:textColor="@color/color_9999"
        app:layout_constraintTop_toBottomOf="@id/print_header_end"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="@dimen/dp_10"
        />
    <ImageView
        android:id="@+id/print_header_img3"
        android:layout_width="@dimen/dp_18"
        android:layout_height="@dimen/dp_18"
        android:src="@mipmap/send_man"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/print_header_orderNum"
        android:layout_marginTop="@dimen/dp_15"
        />
    <TextView
        android:id="@+id/print_header_send_man"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="周润发"
        android:textColor="@color/color_3333"
        android:textSize="@dimen/sp_16"
        app:layout_constraintLeft_toRightOf="@id/print_header_img3"
        app:layout_constraintTop_toBottomOf="@id/print_header_orderNum"
        android:layout_marginTop="@dimen/dp_13"
        android:layout_marginLeft="@dimen/dp_10"
        />
    <TextView
        android:id="@+id/print_header_send_phone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="12535234231"
        android:textSize="@dimen/sp_14"
        android:textColor="@color/color_3333"
        app:layout_constraintLeft_toRightOf="@id/print_header_send_man"
        app:layout_constraintTop_toBottomOf="@id/print_header_orderNum"
        android:layout_marginTop="@dimen/dp_15"
        android:layout_marginLeft="@dimen/dp_8"
        />

    <TextView
        android:id="@+id/print_header_send_locate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="白家市场5栋28号冷冻室"
        android:textColor="@color/color_9999"
        android:textSize="@dimen/sp_14"
        app:layout_constraintLeft_toRightOf="@id/print_header_img3"
        app:layout_constraintTop_toBottomOf="@id/print_header_send_man"
        android:layout_marginTop="@dimen/dp_10"
        android:layout_marginLeft="@dimen/dp_8"
        />
    <ImageView
        android:id="@+id/print_header_img4"
        android:layout_width="@dimen/dp_18"
        android:layout_height="@dimen/dp_18"
        android:src="@mipmap/receive_man"
        app:layout_constraintTop_toBottomOf="@id/print_header_send_locate"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="@dimen/dp_15"
        />
    <TextView
        android:id="@+id/print_header_receive_man"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="李文冷冻"
        android:textColor="@color/color_3333"
        android:textSize="@dimen/sp_16"
        app:layout_constraintLeft_toRightOf="@id/print_header_img4"
        app:layout_constraintTop_toBottomOf="@id/print_header_send_locate"
        android:layout_marginTop="@dimen/dp_13"
        android:layout_marginLeft="@dimen/dp_10"
        />
    <TextView
        android:id="@+id/print_header_receive_phone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="12535234231"
        android:textSize="@dimen/sp_14"
        android:textColor="@color/color_3333"
        app:layout_constraintLeft_toRightOf="@id/print_header_receive_man"
        app:layout_constraintTop_toBottomOf="@id/print_header_send_locate"
        android:layout_marginTop="@dimen/dp_15"
        android:layout_marginLeft="@dimen/dp_8"
        />

    <TextView
        android:id="@+id/print_header_receive_locate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="白家市场5栋28号冷冻室"
        android:textColor="@color/color_9999"
        android:textSize="@dimen/sp_14"
        app:layout_constraintLeft_toRightOf="@id/print_header_img4"
        app:layout_constraintTop_toBottomOf="@id/print_header_receive_man"
        android:layout_marginTop="@dimen/dp_10"
        android:layout_marginLeft="@dimen/dp_8"
        />
    <TextView
        android:id="@+id/print_header_line2"
        android:layout_width="0dp"
        android:layout_height="@dimen/dp_1"
        android:background="@color/color_e0e0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/print_header_receive_locate"
        android:layout_marginTop="@dimen/dp_12"
        />
    <TextView
        android:id="@+id/print_header_word2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/freight"
        android:textColor="@color/color_6666"
        android:textSize="@dimen/sp_14"
        app:layout_constraintLeft_toRightOf="@id/print_header_img4"
        app:layout_constraintTop_toBottomOf="@id/print_header_line2"
        android:layout_marginTop="@dimen/dp_12"
        android:layout_marginLeft="@dimen/dp_10"
        />
    <TextView
        android:id="@+id/print_header_freight"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="已付"
        android:textSize="@dimen/sp_14"
        android:textColor="@color/color_3333"
        app:layout_constraintLeft_toRightOf="@id/print_header_word2"
        app:layout_constraintTop_toBottomOf="@id/print_header_line2"
        android:layout_marginTop="@dimen/dp_12"
        android:layout_marginLeft="@dimen/dp_8"
        />
    <TextView
        android:id="@+id/print_header_word3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/type"
        android:textColor="@color/color_6666"
        android:textSize="@dimen/sp_14"
        app:layout_constraintLeft_toRightOf="@id/print_header_img4"
        app:layout_constraintTop_toBottomOf="@id/print_header_word2"
        android:layout_marginTop="@dimen/dp_12"
        android:layout_marginLeft="@dimen/dp_10"
        />
    <TextView
        android:id="@+id/print_header_type"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="已付"
        android:textSize="@dimen/sp_14"
        android:textColor="@color/color_3333"
        app:layout_constraintLeft_toRightOf="@id/print_header_word3"
        app:layout_constraintTop_toBottomOf="@id/print_header_freight"
        android:layout_marginTop="@dimen/dp_12"
        android:layout_marginLeft="@dimen/dp_8"
        />
    <TextView
        android:id="@+id/print_header_word4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/total"
        android:textColor="@color/color_6666"
        android:textSize="@dimen/sp_14"
        app:layout_constraintLeft_toRightOf="@id/print_header_img4"
        app:layout_constraintTop_toBottomOf="@id/print_header_word3"
        android:layout_marginTop="@dimen/dp_12"
        android:layout_marginLeft="@dimen/dp_10"
        />
    <TextView
        android:id="@+id/print_header_number"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="6件"
        android:textSize="@dimen/sp_14"
        android:textColor="@color/color_3333"
        app:layout_constraintLeft_toRightOf="@id/print_header_word4"
        app:layout_constraintTop_toBottomOf="@id/print_header_type"
        android:layout_marginTop="@dimen/dp_12"
        android:layout_marginLeft="@dimen/dp_8"
        />
    <TextView
        android:id="@+id/print_header_total_detail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="(大件x1  小件x2  中件x3  中小件x5）"
        android:textColor="@color/color_6666"
        android:textSize="@dimen/sp_12"
        app:layout_constraintLeft_toRightOf="@id/print_header_number"
        app:layout_constraintTop_toBottomOf="@id/print_header_type"
        android:layout_marginTop="@dimen/dp_14"
        />
</android.support.constraint.ConstraintLayout>