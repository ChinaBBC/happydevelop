<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_marginLeft="@dimen/dp_12"
    android:layout_marginRight="@dimen/dp_12"
    android:layout_marginTop="@dimen/dp_12"
    android:padding="@dimen/dp_12"
    android:background="@drawable/shape_5dp_59ff">

    <ImageView
        android:id="@+id/print_header_img1"
        android:layout_width="@dimen/dp_20"
        android:layout_height="@dimen/dp_20"
        android:src="@mipmap/select_yes_solid"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />
    <TextView
        android:id="@+id/print_header_word1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/new_client_back"
        android:textSize="@dimen/sp_14"
        android:textColor="@color/color_6666"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />
    <View
        android:id="@+id/print_header_line1"
        android:layout_width="0dp"
        android:layout_height="@dimen/dp_1"
        android:background="@color/color_e0e0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/print_header_word1"
        android:layout_marginTop="@dimen/dp_10"
        />
    <TextView
        android:id="@+id/print_header_start"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="百家市场"
        android:textColor="@color/color_3333"
        android:textSize="@dimen/sp_16"
        android:lines="1"
        android:maxEms="4"
        android:ellipsize="end"
        app:layout_constraintLeft_toRightOf="@id/print_header_img1"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="@dimen/dp_4"

        />
    <ImageView
        android:id="@+id/print_header_img2"
        android:layout_width="@dimen/dp_18"
        android:layout_height="@dimen/dp_10"
        android:src="@mipmap/arrows_double"
        app:layout_constraintLeft_toRightOf="@id/print_header_start"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="@dimen/dp_4"
        android:layout_marginTop="@dimen/dp_6"
        />
    <TextView
        android:id="@+id/print_header_end"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="青羊区(1222223323)"
        android:textColor="@color/color_3333"
        android:textSize="@dimen/sp_16"
        android:lines="1"
        android:ellipsize="end"
        app:layout_constraintLeft_toRightOf="@id/print_header_img2"
        app:layout_constraintRight_toLeftOf="@id/print_header_word1"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="@dimen/dp_4"
        />

    <TextView
        android:id="@+id/print_header_orderNum"
        android:layout_width="wrap_content"
        android:layout_height="17dp"
        android:layout_marginTop="@dimen/dp_10"
        android:text="运单号:1548785494545"
        android:textColor="@color/color_9999"
        android:textSize="@dimen/sp_14"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/print_header_line1" />
    <TextView
        android:id="@+id/print_header_typeAndNum"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/dp_17"
        android:text="冻品2件"
        android:textColor="@color/color_3333"
        android:textSize="@dimen/sp_14"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/print_header_line1"
        android:layout_marginTop="@dimen/dp_10"
        />
    <TextView
        android:id="@+id/print_header_img3"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/dp_18"
        android:text="发货人:"
        android:textSize="@dimen/sp_14"
        android:textColor="@color/color_6666"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/print_header_orderNum"
        android:layout_marginTop="@dimen/dp_8"
        />
    <TextView
        android:id="@+id/print_header_send_man"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="周润发"
        android:textColor="@color/color_3333"
        android:textSize="@dimen/sp_14"
        android:lines="1"
        android:maxEms="4"
        android:ellipsize="end"
        app:layout_constraintLeft_toRightOf="@id/print_header_img3"
        app:layout_constraintTop_toBottomOf="@id/print_header_orderNum"
        android:layout_marginTop="@dimen/dp_8"
        android:layout_marginLeft="@dimen/dp_10"
        />

    <TextView
        android:id="@+id/print_header_send_phone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="13888888888"
        android:textColor="@color/color_3333"
        android:textSize="@dimen/sp_14"
        app:layout_constraintLeft_toRightOf="@id/print_header_send_man"
        app:layout_constraintTop_toBottomOf="@id/print_header_orderNum"
        android:layout_marginTop="@dimen/dp_8"
        android:layout_marginLeft="@dimen/dp_4"
        />
    <TextView
        android:id="@+id/print_header_img4"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/dp_18"
        android:text="收货人:"
        android:textSize="@dimen/sp_14"
        android:textColor="@color/color_6666"
        app:layout_constraintTop_toBottomOf="@id/print_header_send_phone"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="@dimen/dp_8"
        />
    <TextView
        android:id="@+id/print_header_receive_man"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="李文冷冻"
        android:textColor="@color/color_3333"
        android:textSize="@dimen/sp_14"
        android:lines="1"
        android:maxEms="4"
        android:ellipsize="end"
        app:layout_constraintLeft_toRightOf="@id/print_header_img4"
        app:layout_constraintTop_toBottomOf="@id/print_header_send_phone"
        android:layout_marginTop="@dimen/dp_8"
        android:layout_marginLeft="@dimen/dp_10"
        />

    <TextView
        android:id="@+id/print_header_receive_phone"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="1588888888"
        android:textColor="@color/color_3333"
        android:textSize="@dimen/sp_14"
        app:layout_constraintLeft_toRightOf="@id/print_header_receive_man"
        app:layout_constraintTop_toBottomOf="@id/print_header_send_phone"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="@dimen/dp_6"
        android:layout_marginLeft="@dimen/dp_8"
        />
    <TextView
        android:id="@+id/print_header_word3"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/dp_18"
        android:text="收货地址:"
        android:textSize="@dimen/sp_14"
        android:textColor="@color/color_6666"
        app:layout_constraintTop_toBottomOf="@id/print_header_receive_phone"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginTop="@dimen/dp_8"
        />
    <TextView
        android:id="@+id/print_header_received_address"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="这就是收货地址"
        android:textColor="@color/color_3333"
        android:textSize="@dimen/sp_14"
        app:layout_constraintLeft_toRightOf="@id/print_header_word3"
        app:layout_constraintTop_toBottomOf="@id/print_header_receive_phone"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="@dimen/dp_8"
        android:layout_marginLeft="@dimen/dp_8"
        />
    <TextView
        android:id="@+id/print_header_word2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="运费:"
        android:textColor="@color/color_6666"
        android:textSize="@dimen/sp_14"
        app:layout_constraintRight_toLeftOf="@+id/print_header_freight"
        app:layout_constraintTop_toBottomOf="@id/print_header_orderNum"
        android:layout_marginTop="@dimen/dp_8"
        android:layout_marginRight="@dimen/dp_6"
        />
    <TextView
        android:id="@+id/print_header_freight"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="40元(到付)"
        android:textSize="@dimen/sp_14"
        android:textColor="@color/color_3333"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/print_header_orderNum"
        android:layout_marginTop="@dimen/dp_8"
        />
    <TextView
        android:id="@+id/print_header_remark"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="备注：请一定送货到收货人手里！谢谢！"
        android:textColor="@color/color_3333"
        android:textSize="@dimen/sp_12"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/print_header_received_address"
        android:padding="@dimen/dp_8"
        android:layout_marginTop="@dimen/dp_6"
        android:background="@color/color_f5f1"
        />
    <!--<TextView
        android:id="@+id/print_header_word3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/type"
        android:textColor="@color/color_6666"
        android:textSize="@dimen/sp_14"
        app:layout_constraintLeft_toRightOf="@id/print_header_img4"
        app:layout_constraintTop_toBottomOf="@id/print_header_word2"
        android:layout_marginTop="@dimen/dp_6"
        android:layout_marginLeft="@dimen/dp_10"
        />
    <TextView
        android:id="@+id/print_header_type"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="已付"
        android:textSize="@dimen/sp_14"
        android:textColor="@color/color_3333"
        app:layout_constraintLeft_toRightOf="@id/print_header_word3"
        app:layout_constraintTop_toBottomOf="@id/print_header_freight"
        android:layout_marginTop="@dimen/dp_6"
        android:layout_marginLeft="@dimen/dp_8"
        />
    <TextView
        android:id="@+id/print_header_word4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/total"
        android:textColor="@color/color_6666"
        android:textSize="@dimen/sp_14"
        app:layout_constraintLeft_toRightOf="@id/print_header_img4"
        app:layout_constraintTop_toBottomOf="@id/print_header_word3"
        android:layout_marginTop="@dimen/dp_6"
        android:layout_marginLeft="@dimen/dp_10"
        />
    <TextView
        android:id="@+id/print_header_number"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="6件"
        android:textSize="@dimen/sp_14"
        android:textColor="@color/color_3333"
        app:layout_constraintLeft_toRightOf="@id/print_header_word4"
        app:layout_constraintTop_toBottomOf="@id/print_header_type"
        android:layout_marginTop="@dimen/dp_6"
        android:layout_marginLeft="@dimen/dp_8"
        />
    <TextView
        android:id="@+id/print_header_total_detail"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="(大件x1  小件x2  中件x3  中小件x5）"
        android:textColor="@color/color_6666"
        android:textSize="@dimen/sp_12"
        app:layout_constraintLeft_toRightOf="@id/print_header_number"
        app:layout_constraintTop_toBottomOf="@id/print_header_type"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="@dimen/dp_8"
        />-->
</android.support.constraint.ConstraintLayout>